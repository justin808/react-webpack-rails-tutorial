{
  "name": "react-webpack-rails-tutorial",
  "sources": [
    {
      "dir": "client/app/bundles/comments/src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "bsc-flags": [
    "-open JsonCombinators",
    "-open Belt"
  ],
  "suffix": ".bs.mjs",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators"
  ],
  "jsx": {
  "version": 4,
    "mode": "automatic"
  }
}
